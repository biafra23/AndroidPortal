<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Timber name="Timber">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </Timber>
    </Appenders>
    <Loggers>
        <Root level="debug">
            <AppenderRef ref="Timber"/>
        </Root>
    </Loggers>
</Configuration>